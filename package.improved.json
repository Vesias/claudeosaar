{
  "name": "claudeosaar",
  "version": "2.3.0",
  "description": "AI Development Workspace OS with containerized environments and Claude integration",
  "author": "ClaudeOSaar Team",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development ts-node -r tsconfig-paths/register --transpile-only src/api/server.ts",
    "dev:python": "cd src/api && uvicorn main:app --reload --host 0.0.0.0 --port 6600",
    "dev:ui": "next dev -p 6601",
    "dev:mcp": "./dev-mcp.sh",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:ui\" \"npm run dev:mcp\"",
    "build": "tsc -p tsconfig.build.json && next build",
    "build:ui": "next build",
    "build:docker": "docker compose -f containers/docker-compose.yaml build",
    "start": "NODE_ENV=production node dist/api/server.js",
    "start:ui": "next start -p 6601",
    "start:all": "./start.sh",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:ci": "eslint . --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "security:audit": "npm audit && npm audit fix",
    "security:scan": "snyk test",
    "clean": "rm -rf .next dist coverage .tsbuildinfo",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "release": "standard-version",
    "docker:up": "docker compose -f containers/docker-compose.yaml up -d",
    "docker:down": "docker compose -f containers/docker-compose.yaml down",
    "docker:logs": "docker compose -f containers/docker-compose.yaml logs -f",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "analyze": "ANALYZE=true npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@prisma/client": "^5.10.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.334.0",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redis": "^4.6.13",
    "stripe": "^14.20.0",
    "winston": "^3.11.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "@playwright/test": "^1.41.2",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/nextjs": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-unicorn": "^51.0.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prisma": "^5.10.0",
    "snyk": "^1.1279.0",
    "standard-version": "^9.5.0",
    "storybook": "^7.6.17",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai",
    "claude",
    "workspace",
    "docker",
    "typescript",
    "nextjs",
    "containerization",
    "development-environment"
  ]
}