{
  "name": "claudeosaar",
  "version": "2.1.0",
  "description": "ClaudeOSaar - AI Development Workspace OS",
  "main": "dist/index.js",
  "bin": {
    "claudeosaar": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/api/server.ts --port 6600",
    "dev:python": "cd src && python3 -m uvicorn api.main:app --reload --port 6600",
    "dev:ui": "next dev -p 6601",
    "dev:mcp": "./dev-mcp.sh",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:ui\" \"npm run dev:mcp\"",
    "build": "tsc",
    "build:ui": "next build",
    "start": "node dist/api/server.js",
    "start:all": "./start.sh",
    "cli": "tsx src/cli/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker-compose -f containers/docker-compose.yaml build",
    "docker:up": "docker-compose -f containers/docker-compose.yaml up -d",
    "docker:down": "docker-compose -f containers/docker-compose.yaml down",
    "release": "tsx .claude/commands/next-release-planner.ts"
  },
  "keywords": [
    "claude",
    "ai",
    "workspace",
    "development",
    "containers"
  ],
  "author": "ClaudeOSaar Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/handlebars": "^4.0.40",
    "@types/node": "^20.11.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "bcrypt": "^6.0.0",
    "commander": "^11.1.0",
    "cookie-parser": "^1.4.7",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "docker-compose": "^0.24.7",
    "dockerode": "^4.0.6",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.3.2",
    "nodemailer": "^7.0.3",
    "node-pty": "^1.0.0",
    "pg": "^8.11.3",
    "pgvector": "^0.1.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.15.3",
    "stripe": "^18.1.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ws": "^8.18.2",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "xterm-addon-webgl": "^0.16.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "@types/dockerode": "^3.3.38",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.7",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
