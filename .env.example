# ClaudeOSaar Environment Configuration
# Copy this file to .env and fill in your values

# API Keys
ANTHROPIC_API_KEY=your_claude_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
HF_TOKEN=your_huggingface_token_here

# Database Configuration
VECTOR_DB_PASSWORD=generate_secure_password_here
API_DB_PASSWORD=generate_secure_password_here
DB_HOST=localhost
DB_PORT=5432
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/claudeosaar
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20

# Redis (for sessions, caching, and pub/sub)
REDIS_URL=redis://localhost:6379

# Security
JWT_SECRET=generate_jwt_secret_here
ENCRYPTION_KEY=generate_32_byte_key_here
SESSION_SECRET=generate_session_secret_here
SECRET_KEY=your_secure_secret_key_change_in_production

# CORS and WebSockets
CORS_ORIGINS=http://localhost:6601,https://claudeosaar.saarland
ALLOWED_HOSTS=*
WEBSOCKET_ALLOWED_ORIGINS=*

# Billing (Stripe)
STRIPE_SECRET_KEY=sk_test_your_stripe_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key
STRIPE_PRO_PRICE_ID=price_123abc
STRIPE_ENTERPRISE_PRICE_ID=price_456def

# GitHub Integration
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here

# Monitoring
GRAFANA_PASSWORD=secure_admin_password_here
PROMETHEUS_RETENTION=30d

# Application
NODE_ENV=development
DEBUG=false
HOST=0.0.0.0
PORT=6600
API_URL=http://localhost:6600
FRONTEND_URL=http://localhost:6601
WEBSOCKET_URL=ws://localhost:6600/ws

# Container Configuration
CONTAINER_RUNTIME=docker
DOCKER_REGISTRY=ghcr.io/claudeosaar
CLAUDEOSAAR_VERSION=2.2.1-dev
DOCKER_SOCKET=/var/run/docker.sock
DOCKER_NETWORK=claudeosaar_network

# MCP Configuration
MCP_CONFIG_PATH=./.claude/mcp-server/config.json
MCP_SERVER_PORT=6602

# Resource Limits (per tier)
FREE_TIER_RAM_MB=512
FREE_TIER_CPU=0.5
FREE_TIER_STORAGE_GB=5

PRO_TIER_RAM_MB=2048
PRO_TIER_CPU=2
PRO_TIER_STORAGE_GB=50

ENTERPRISE_TIER_RAM_MB=8192
ENTERPRISE_TIER_CPU=4
ENTERPRISE_TIER_STORAGE_GB=100

# Networking
INTERNAL_NETWORK_SUBNET=172.20.0.0/16
PUBLIC_DOMAIN=agentland.saarland
API_SUBDOMAIN=api.agentland.saarland
APP_SUBDOMAIN=app.agentland.saarland

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=noreply@claudeosaar.saarland
SMTP_PASS=your_smtp_password
SMTP_FROM=ClaudeOSaar <noreply@claudeosaar.saarland>
SENDGRID_API_KEY=SG.your_sendgrid_key

# Feature Flags
ENABLE_MULTI_AGENT=true
ENABLE_REAL_TIME_COLLAB=false
ENABLE_EDGE_DEPLOYMENT=false
ENABLE_KUBERNETES=false
ENABLE_METRICS=true
ENABLE_WEBSOCKETS=true

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# SSL
USE_SSL=false
SSL_KEYFILE=/path/to/key.pem
SSL_CERTFILE=/path/to/cert.pem

# Admin
ADMIN_EMAIL=admin@claudeosaar.saarland
ADMIN_PASSWORD=changeme

# Storage
UPLOAD_DIR=/tmp/uploads
MAX_UPLOAD_SIZE=104857600  # 100MB